<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <Version>2.2.0-beta3</Version>
    <PackageProjectUrl>https://github.com/AlexanderOnTesting/NetCoreWebDriverFactory</PackageProjectUrl>
    <PackageLicenseExpression>Apache-2.0</PackageLicenseExpression>
    <Company>AlexanderOnTest.tech</Company>
    <Authors>Alexander Dunn</Authors>
    <Product>WebDriverFactoryNunitConfig</Product>
    <PackageTags>Selenium, WebDriverFactory, WebDriver, Selenium WebDriver, DotNet Core, .net Core., NetCoreWebDriverFactory</PackageTags>
    <Copyright>Copyright 2019 Alexander Dunn</Copyright>
    <Description>Pick up NetCoreWebDriverFactory configuration for Nunit 3 test projects from runsettings files and with local file overrides.</Description>
    <PackageReleaseNotes>v2.2.0-Beta3
v2.2.0-Beta2
Initial release: version number matched to concurrent release NetCoreWebDriverFactory</PackageReleaseNotes>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <PackageId>Selenium.WebDriver.WebDriverFactoryNunitConfig</PackageId>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="NUnit" Version="3.11.0" />
    <PackageReference Include="Selenium.WebDriver.NetCoreWebDriverFactory" Version="2.2.0-beta3" />
  </ItemGroup>

</Project>
